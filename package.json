{
  "name": "ramda-matrix",
  "version": "2.0.0",
  "description": "Ramda-based library for working with matrices in JavaScript",
  "author": "Max Patiiuk",
  "license": "MIT",
  "keywords": [
    "ramda",
    "matrix"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxpatiiuk/ramda-matrix"
  },
  "homepage": "https://github.com/maxpatiiuk/ramda-matrix/#readme",
  "bugs": {
    "url": "https://github.com/maxpatiiuk/ramda-matrix/issues"
  },
  "type": "module",
  "private": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint --fix . && prettier --write .",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "watch": "tsup --watch"
  },
  "peerDependencies": {
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@maxpatiiuk/prettier-config": "^2.0.0",
    "@types/ramda": "^0.30.0",
    "eslint": "^9.5.0",
    "prettier": "^3.3.2",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2"
  },
  "prettier": "@maxpatiiuk/prettier-config"
}
